{:paths   ["src" "resources"]
 :deps    {org.clojure/clojure       {:mvn/version "1.11.1"}
           org.clojure/core.async    {:mvn/version "1.6.673"}
           org.clojure/tools.logging {:mvn/version "1.2.4"}
           org.clojure/data.json     {:mvn/version "2.4.0"}
           org.clojure/data.xml      {:mvn/version "0.2.0-alpha8"}

           org.eclipse.jetty/jetty-client {:mvn/version "11.0.18"}
           org.eclipse.jetty/jetty-http   {:mvn/version "11.0.18"}
           org.eclipse.jetty/jetty-util   {:mvn/version "11.0.18"}
           }
 :aliases {:update-versions {:extra-paths ["build/src"]
                             :main-opts   ["-m" "cognitect.aws.version-updater"]}
           :dev             {:extra-paths ["dev/src" "dev/resources" "test/src" "test/resources" "examples" "examples/resources" "dev/resources"]
                             :extra-deps  {commons-io/commons-io         {:mvn/version "2.11.0"}
                                           org.clojure/test.check        {:mvn/version "1.1.1"}
                                           org.slf4j/slf4j-reload4j      {:mvn/version "2.0.3"}
                                           http-kit/http-kit             {:mvn/version "2.6.0"}
                                           com.amazonaws/aws-java-sdk    {:mvn/version "1.12.344"}
                                           com.cognitect.aws/endpoints   {:mvn/version "1.1.12.504"}
                                           com.cognitect.aws/autoscaling {:mvn/version "847.2.1365.0"}
                                           com.cognitect.aws/dynamodb    {:mvn/version "848.2.1413.0"}
                                           com.cognitect.aws/ec2         {:mvn/version "848.2.1413.0"}
                                           com.cognitect.aws/iam         {:mvn/version "848.2.1413.0"}
                                           com.cognitect.aws/lambda      {:mvn/version "848.2.1413.0"}
                                           com.cognitect.aws/s3          {:mvn/version "848.2.1413.0"}
                                           com.cognitect.aws/ssm         {:mvn/version "848.2.1413.0"}
                                           com.cognitect.aws/sts         {:mvn/version "847.2.1387.0"}}}
           :test            {:extra-deps {io.github.cognitect-labs/test-runner {:git/tag "v0.5.1" :git/sha "dfb30dd"}}
                             :main-opts  ["-m" "cognitect.test-runner"
                                          "-e" "integration"]}
           :test-integration {:extra-deps {io.github.cognitect-labs/test-runner {:git/tag "v0.5.1" :git/sha "dfb30dd"}}
                              :main-opts  ["-m" "cognitect.test-runner"
                                           "-i" "integration"]}}}
